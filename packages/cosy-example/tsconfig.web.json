{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/ui/env.d.ts",
    "src/ui/**/*",
    "src/ui/**/*.vue",
    "src/types/**/*",
    "vite.config.mjs",
    "../plugins/plugin-chat/absolute-asteroid/src/ChatView.vue"
  ],
  "compilerOptions": {
    "composite": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@renderer/*": [
        "src/ui/*"
      ],
      "@components/*": [
        "src/ui/components/*"
      ],
      "@renderer/assets/*": [
        "src/ui/assets/*"
      ],
      "@utils/*": [
        "src/ui/utils/*"
      ],
      "@renderer/stores/*": [
        "src/ui/stores/*"
      ],
      "@renderer/hooks/*": [
        "src/ui/hooks/*"
      ],
      "@renderer/services/*": [
        "src/ui/services/*"
      ],
      "@ipc/*": [
        "src/ui/ipc/*"
      ],
      "@renderer/types/*": [
        "src/ui/types/*"
      ],
    }
  },
}