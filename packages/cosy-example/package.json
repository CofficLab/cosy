{
  "name": "@coffic/cosy-example",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "type": "module",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://coffic.zone.id",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "cosy": "cosy",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac --publish never",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepseek": "^0.2.15",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/vue": "^1.2.12",
    "@coffic/active-app-monitor": "^1.0.2",
    "@coffic/buddy-it": "^1.1.3",
    "@coffic/cosy-framework": "workspace:*",
    "@coffic/cosy-keyboard": "workspace:*",
    "@coffic/cosy-db-ui": "workspace:*",
    "@coffic/cosy-ui": "^0.8.29",
    "@coffic/key-listener": "^1.0.4",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@remixicon/vue": "^4.6.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/marked": "^5.0.2",
    "@vueuse/core": "^13.5.0",
    "ai": "^4.3.17",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "daisyui": "^5.0.46",
    "dotenv": "^17.2.0",
    "electron-log": "^5.4.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.483.0",
    "marked": "^15.0.12",
    "mitt": "^3.0.1",
    "openai": "^4.104.0",
    "pinia": "^3.0.3",
    "tar": "^7.4.3",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "vue-router": "^4.5.1",
    "yaml": "^2.8.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/cors": "^2.8.19",
    "@types/node": "^22.16.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-vue": "^10.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue": "^3.5.17",
    "vue-tsc": "^2.2.12"
  }
}
