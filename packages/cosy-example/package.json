{
  "name": "buddy",
  "version": "1.4.14",
  "description": "An Electron application with Vue and TypeScript",
  "type": "module",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://coffic.zone.id",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "cosy": "cosy",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac --publish never",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.4",
    "@ai-sdk/deepseek": "^0.2.5",
    "@ai-sdk/openai": "^1.3.6",
    "@ai-sdk/vue": "^1.2.4",
    "@coffic/active-app-monitor": "^1.0.2",
    "@coffic/buddy-it": "^1.0.0",
    "@coffic/cosy-framework": "workspace:*",
    "@coffic/cosy-keyboard": "workspace:*",
    "@coffic/cosy-ui": "^0.8.17",
    "@coffic/key-listener": "^1.0.3",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@remixicon/vue": "^4.6.0",
    "@tailwindcss/vite": "^4.0.15",
    "@types/marked": "^5.0.2",
    "@vueuse/core": "^13.1.0",
    "ai": "^4.2.10",
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "daisyui": "^5.0.12",
    "electron-log": "^5.3.3",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.483.0",
    "marked": "^15.0.7",
    "openai": "^4.91.1",
    "pinia": "^3.0.1",
    "tar": "^7.4.3",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "vue-router": "^4.5.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/cors": "^2.8.19",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "autoprefixer": "^10.4.21",
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
