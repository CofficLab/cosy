{
  "name": "@coffic/cosy-db-ui",
  "version": "1.0.0",
  "description": "Database UI management extension for Cosy Framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "cosy-extension",
    "cosy-framework",
    "database",
    "ui",
    "management"
  ],
  "cosy": {
    "type": "command",
    "commands": [
      {
        "name": "ui",
        "description": "Open database UI in browser",
        "configure": "dist/commands/ui-command.js"
      }
    ]
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "author": "Coffic Team",
  "license": "MIT",
  "dependencies": {
    "@coffic/cosy-framework": "workspace:*",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "express": "^4.18.2",
    "ora": "^8.0.1",
    "open": "^10.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.8",
    "jest": "^29.5.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@coffic/cosy-framework": "^1.0.0"
  }
}
